{
    "name": "service-hub-proxy",
    "version": "1.0.0",
    "scripts": {
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "start:backend": "cd Service-hub-main/backend && npm start",
        "start:frontend": "cd Service-hub-main/frontend && npm run dev",
        "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "install:all": "cd Service-hub-main && npm install && cd backend && npm install && cd ../frontend && npm install"
    },
    "dependencies": {
        "firebase": "^12.0.0",
        "react-router-dom": "^7.7.1"
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}